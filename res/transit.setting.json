{
    "DefaultEditor": "Neovim",
    "Editor": {
        "Neovim": {
            "Command": "start nvim \"<path><line>\"",
            "ElevatedCommand": "sudo nvim \"<path>\"<line>",
            "GotoLineSequence": " +"
        },
        "VsCode": {
            "Command": "code --goto \"<path><line>\"",
            "ElevatedCommand": "sudo code --goto \"<path><line>\"",
            "GotoLineSequence": ":"
        },
        "Other": {
            "Command": "start <app> \"<path>\"",
            "ElevatedCommand": "sudo <app> \"<path>\"",
            "GotoLineSequence": ":"
        }
    }
}
