{
    "Link": {
        "Notebook": "/note/*.md"
    },
    "UxWrite": {
        "ImageProcessAppPath": "magick",
        "DefaultExtension": "png",
        "DefaultImageSize": 32,
        "ResourceDir": "res",
        "Delimiter": "!!",
        "DateTimeFormat": "yyyy-MM-dd-HHmmss",
        "DateTimePattern": "\\d{4}_\\d{2}_\\d{2}_\\d{6}",
        "SplitDelimiter": ":"
    }
}
